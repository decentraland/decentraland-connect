{
  "name": "decentraland-connect",
  "version": "0.0.0-development",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "esm",
    "cjs"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json -e '*.json' -c tslint.json 'src/**/*.ts{,x}'",
    "lint:fix": "tslint  --fix --project tsconfig.json -e '*.json' -c tslint.json 'src/**/*.ts{,x}'",
    "postbuild": "node scripts/postbuild.js",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:report": "jest --coverage",
    "semantic-release": "semantic-release",
    "commitmsg": "validate-commit-msg"
  },
  "dependencies": {
    "@dcl/schemas": "^15.1.2",
    "@magic-ext/oauth2": "^9.19.0",
    "@walletconnect/ethereum-provider": "^2.23.1",
    "@walletconnect/modal": "^2.7.0",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.3",
    "@web3-react/walletlink-connector": "^6.2.13",
    "magic-sdk": "^27.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.18.9",
    "@types/sinon": "^9.0.9",
    "@web3-react/types": "^6.0.7",
    "dcl-tslint-config-standard": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^29.7.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.3",
    "sinon": "^9.2.2",
    "ts-jest": "^29.2.5",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.9.5",
    "validate-commit-message": "^3.0.1"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decentraland/decentraland-connect.git"
  }
}
